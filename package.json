{
  "name": "nginflection",
  "main": "index.js",
  "version": "1.1.1",
  "homepage": "https://github.com/konsumer/ngInflection",
  "repository": {
    "type": "git",
    "url": "https://github.com/konsumer/ngInflection.git"
  },
  "bugs": {
    "url": "https://github.com/konsumer/ngInflection/issues"
  },
  "authors": [
    "David Konsumer <konsumer@jetboystudio.com>"
  ],
  "description": "Angular filters for inflection",
  "keywords": [
    "angular",
    "inflection",
    "inflections",
    "inflection-js",
    "pluralize",
    "singularize",
    "inflect",
    "camelize",
    "underscore",
    "humanize",
    "capitalize",
    "dasherize",
    "titleize",
    "demodulize",
    "tableize",
    "classify",
    "foreign_key",
    "ordinalize"
  ],
  "license": "MIT",
  "dependencies": {
    "angular": ">1.0.0",
    "inflection": "~1.4.0"
  },
  "devDependencies": {
    "browserify": "^8.1.1",
    "uglify-js": "^2.4.16"
  },
  "scripts": {
    "build": "browserify -s ngInflection -x inflection -x angular -o ngInflection.js .",
    "production": "npm run build && uglifyjs ngInflection.js -o ngInflection.min.js",
    "deploy": "npm run production && node bump.js && npm publish"
  }
}